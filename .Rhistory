if(is.null(Metadata$geneAnnotation)){ attributes(Metadata)$Data.Type <-  c(attributes(Metadata)$Data.Type, "geneAnnot")
attributes(Metadata)$Export <- c(attributes(Metadata)$Export,"Yes") } else {
}
